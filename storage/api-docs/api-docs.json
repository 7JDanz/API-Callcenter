        "/pais": {
            "get": {
                "tags": [
                    "Pais"
                ],
                "summary": "Lista todos los Paises",
                "description": "Retorna todos los paises",
                "operationId": "getPaisList",
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        },
        "/restaurante/IDRestaurante/{IDRestaurante}": {
            "get": {
                "tags": [
                    "Restaurante"
                ],
                "summary": "Lista restaurantes por IDRestaurante",
                "description": "Retorna restaurante por IDRestaurante",
                "operationId": "getRestauranteId",
                "parameters": [
                    {
                        "name": "IDRestaurante",
                        "in": "path",
                        "description": "Puede buscar por uno ó varios Ids separados por coma /4 ó /4,20",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        },
        "/restaurante/IDCadena/{IDCadena}": {
            "get": {
                "tags": [
                    "Restaurante"
                ],
                "summary": "Lista restaurantes por IDCadena",
                "description": "Retorna restaurante por IDCadena",
                "operationId": "getRestaurantePorIDCadena",
                "parameters": [
                    {
                        "name": "IDCadena",
                        "in": "path",
                        "description": "Puede buscar por uno ó varios Ids separados por coma /10 ó /12,10",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Unauthenticated"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                }
            }
        },
        "/sanctum/csrf-cookie": {
            "get": {
                "tags": [
                    "Usuario"
                ],
                "summary": "Obtener Token",
                "responses": {
                    "204": {
                        "description": "No Content"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                }
            }
        },
        "/login": {
            "post": {
                "tags": [
                    "Usuario"
                ],
                "summary": "Login de usuarios, referirse a la siguiente documentación:https://laravel.com/docs/8.x/sanctum#spa-authentication",
                "parameters": [
                    {
                        "name": "X-XSRF-TOKEN",
                        "in": "header"
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "usuario": {
                                        "type": "string",
                                        "example": "fsierra"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "SVucd2fYX5KVaiL"
                                    },
                                    "pais": {
                                        "type": "int",
                                        "example": 1
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Respuesta correcta",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "two_factor": {
                                            "description": "Si retorna OK ya se encuentra logueado",
                                            "type": "boolean",
                                            "example": "false"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "The given data was invalid."
                                        },
                                        "errors": {
                                            "description": "Lista de errores ",
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "error": {
                                                        "type": "string",
                                                        "example": "These credentials do not match our records."
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "name": "bearerAuth",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "security": [
